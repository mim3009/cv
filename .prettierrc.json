{
	"printWidth": 130,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"printWidth": 1000
			}
		},
		{
			"files": "*.json",
			"options": {
				"singleQuote": false
			}
		}
	]
}
